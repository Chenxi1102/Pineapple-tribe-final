# read in the UFO data
UFO_data <- read.delim("../data/UFOCoords.csv.bz2")
# Define server logic required to draw a histogram
# draw a historam according to user input
my_server <- function(input, output) {
output$finalPlot <- renderPlot({
ggplot(UFO_data, aes(x = get(input$value), y = get(input$value2))) + geom_point(color = "green") +
xlab(input$value) + ylab(input$value2) + ggtitle(paste0(input$value, " vs. ", input$value2)) +
theme(axis.title = element_text(size = 15), plot.title = element_text(size = 20, face = "bold"))
})
}
